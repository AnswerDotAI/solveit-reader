{
  "manifest_version": 3,
  "name": "Solveit Reader",
  "version": "2.0",
  "description": "Import any webpage into Solveit for reading and annotation",
  "permissions": ["activeTab", "storage", "cookies", "scripting"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [{
    "resources": ["turndown.js", "turndown-plugin-gfm.js"],
    "matches": ["<all_urls>"]
  }]
}
